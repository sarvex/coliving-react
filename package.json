{
  "name": "coliving",
  "author": {
    "name": "Sarvex Jatasra",
    "email": "sarvex@jatasra.com",
    "url": "https://jatasra.com"
  },
  "homepage": "https://coliving.ai",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mui/material": "5.13.3",
    "@mui/icons-material": "5.11.16",
    "@types/googlemaps": "3.43.3",
    "@types/markerclustererplus": "2.1.33",
    "ajv": "8.12.0",
    "animate.css": "4.1.1",
    "classnames": "2.3.2",
    "core-js": "3.30.2",
    "history": "5.3.0",
    "moment": "2.29.4",
    "node-sass": "9.0.0",
    "nouislider": "15.7.0",
    "prop-types": "15.8.1",
    "react": "^18.0.0",
    "react-animate-on-scroll": "^2.1.5",
    "react-datetime": "^3.0.0",
    "react-dom": "^18.0.0",
    "react-google-maps": "^9.4.5",
    "react-image-gallery": "^1.0.0",
    "react-router": "^6.0.0",
    "react-router-dom": "^6.0.0",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.29.0",
    "react-swipeable-views": "^0.14.0",
    "react-tagsinput": "^3.19.0"
  },
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.22.1",
    "@babel/preset-env": "7.22.4",
    "@babel/preset-react": "7.22.3",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "3.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext=js,jsx --fix;  exit 0",
    "clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "sass:compile": "node-sass src/assets/scss/material-kit-pro-react.scss src/assets/css/material-kit-pro-react.css",
    "sass:minify": "node-sass src/assets/scss/material-kit-pro-react.scss src/assets/css/material-kit-pro-react.min.css --output-style compressed",
    "sass:map": "node-sass src/assets/scss/material-kit-pro-react.scss src/assets/css/material-kit-pro-react.css --source-map true"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "globals": {
      "use": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react",
      "react-app"
    ],
    "plugins": [
      "prettier",
      "react"
    ],
    "rules": {
      "implicit-arrow-linebreak": "off",
      "comma-dangle": "off",
      "indent": "off",
      "no-trailing-spaces": "off",
      "no-param-reassign": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    }
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "semi": false,
    "useTabs": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "endOfLine": "cr"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
